{
  "apps": [
    {
      "name": "wwbb", //名称
      "script": "./bin/www", //程序入库
      "cwd": "./", //根目录
      "watch": true, //需要监控的目录
      "ignore_watch": [ // 从监控目录中排除
        "node_modules",
        "logs",
        "public"
    ],
      "error_file": "./logs/app-err.log", //错误输出日志
      "out_file": "./logs/app-out.log", //日志
      "log_date_format": "YYYY-MM-DD HH:mm Z" //日期格式
    }
  ]
}
